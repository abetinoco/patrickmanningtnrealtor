{
  "redirects": [
    {
      "source": "/property-listings/sitemap",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/property-listings/:path*",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
